{
  "name": "element-ext",
  "version": "0.1.0",
  "main": "./dist/element-ext.common.js",
  "scss": "./src/assets/scss/index.scss",
  "publishConfig": {
    "registry": "http://localhost:8081/repository/npm-hosted/"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --open ./examples/index.js",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name element-ext ./src/index.js",
    "build-scss": "vue-cli-service build --target lib ./src/assets/scss/index.scss",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sortablejs": "^1.10.0-rc3",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.23.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
